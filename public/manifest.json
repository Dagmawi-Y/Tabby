{
  "manifest_version": 3,
  "name": "Tabby",
  "version": "1.0.0",
  "description": "Meow-nage your tab clutter.",
  "permissions": ["tabs", "storage", "scripting"],
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "32": "tabby.png"
  }
}
