{
  "manifest_version": 3,
  "name": "Tabby",
  "version": "1.0.0",
  "description": "Meow-nage your tab clutter.",
  "permissions": ["tabs", "storage", "scripting", "downloads", "notifications"],
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["<all_urls>"],
  "icons": {
    "32": "tabby.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
